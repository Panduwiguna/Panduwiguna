
import java.util.Scanner;

public class nilaiCount {
    public static void main(String[] args) {
        Double angka;
        String nilai;
        Scanner sc = new Scanner(System.in);
        System.out.print("Masukkan nilai: ");
        angka = sc.nextDouble();

        if(angka > 75){
            nilai = "lulus";
  
        }else{
             nilai = "Tidak Lulus";
        }
        System.out.println(" Keterangan: " +nilai);
    sc.close();
    }
}
